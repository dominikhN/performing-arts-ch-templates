  _:genid-8a9a4b19bb5a4e8fa8598866b1031a38-9mxa3k a <http://spinrdf.org/sp#Query>;
    <http://spinrdf.org/sp#text> """SELECT ?value ?label WHERE {
  ?value a crm:E22_Man-Made_Object ;
    crm:P2_has_type spav:dwmkn ;
    rdfs:label ?venue_label .
  SERVICE <http://www.bigdata.com/rdf/search#search> {
    ?venue_label bds:search \"?token\" ;
    bds:minRelevance \"0.1\" ;
    bds:relevance ?score ; 
    bds:matchAllTerms \"true\" .
  }
  FILTER(CONTAINS(STR(?value), \"/o/\"))
  OPTIONAL {
   	?parent crm:P46_is_composed_of ?value ;
            rdfs:label ?parent_label .
	FILTER(LANG(?parent_label) = \"\" || LANGMATCHES(LANG(?parent_label), ?__userPreferredLanguage__))
  }
  BIND(IF(BOUND(?parent_label), CONCAT(?venue_label, \" (\", ?parent_label, \")\"), ?venue_label) AS ?label)  
  FILTER(LANG(?venue_label) = \"\" || LANGMATCHES(LANG(?venue_label), ?__userPreferredLanguage__))
  BIND(REPLACE(STR(?token), \"\\\\*\", \"\") AS ?clean_token) .
  FILTER(CONTAINS(LCASE(STR(?venue_label)), LCASE(?clean_token)))
} LIMIT 20""" .
  
