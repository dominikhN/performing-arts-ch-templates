  _:genid-fc3c18383cdb47428821baae704a5434-tu8aka a <http://spinrdf.org/sp#Query>;
    <http://spinrdf.org/sp#text> """SELECT $value ?unidentified WHERE {
  $subject crm:P107_has_current_or_former_member $value .
  BIND(IF(CONTAINS(STR($value), \"/u/\"), \"true\", \"\") AS ?unidentified)
}""" .
  
  _:genid-fc3c18383cdb47428821baae704a5434-uyaxyo a <http://spinrdf.org/sp#Query>;
    <http://spinrdf.org/sp#text> """SELECT ?value ?label WHERE {
  {
    SELECT DISTINCT ?value WHERE {
      ?value a crm:E21_Person ;
        crm:P131_is_identified_by [ rdf:value ?l ] .
      FILTER(CONTAINS(STR(?value), \"/a/\"))
      SERVICE <http://www.bigdata.com/rdf/search#search> {
        ?l bds:search ?__token__ ;
        bds:minRelevance \"0.1\" ;
        bds:relevance ?score ; 
        bds:matchAllTerms \"true\" .
      }
    }
  }
  ?value crm:P131_is_identified_by [
    crm:P2_has_type spav:yatcx ;
    rdf:value ?l ] .
  OPTIONAL {
    ?value crm:P3_has_note [
      crm:P2_has_type spav:eoept ;
      rdf:value ?note ]
  }
  BIND(IF(BOUND(?note), CONCAT(?l, \" (\", ?note, \")\"), ?l) AS ?label)
} LIMIT 10""" .
  
