  _:genid-2f04ccfd308448afb0a02c80793ad28b-affkil <http://spinrdf.org/sp#text> """SELECT DISTINCT ?value ?label WHERE {
  # apparently subject has to be used once before the filter
  $subject rdfs:label ?x .
  ?value a crm:E21_Person ;
    crm:P131_is_identified_by [ rdf:value ?l ] .
  FILTER(CONTAINS(STR(?value), \"/a/\"))
  FILTER(NOT EXISTS { $subject crm:P107_has_current_or_former_member $value . })
  FILTER(REGEX(STR(?l), \"?token\", \"i\"))
  OPTIONAL { ?value crm:P3_has_note [ rdf:value ?note ] . }
  BIND(IF(BOUND(?note), CONCAT(?l, \" (\", ?note, \")\"), ?l) AS ?label)
} LIMIT 10""" .
  
